    "react-rnd": "^10.3.4"
  },
  "devDependencies": {
    "@ant-design/icons": "^4.6.2",
    "@ant-design/pro-layout": "^6.5.0",
    "antd": "^4.16.9",
    "umi": "^3.5.14",
    "@types/lodash": "^4.14.172",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.14",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
