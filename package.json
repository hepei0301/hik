{
  "private": false,
  "name": "hik",
  "version": "1.0.0",
  "description": "hik project",
  "main": "./index.ts",
  "module": "./index.ts",
  "jsnext:main": "./index.ts",
  "typings": "./index.ts",
  "files": [
    "src",
    "index.ts"
  ],
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
